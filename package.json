{
  "name": "slide-element",
  "version": "1.0.1",
  "description": "A Promise-based, jQuery-reminiscent collection of functions to help hide and show elements in a sliding fashion.",
  "main": "dist/index.modern.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/types.d.ts",
  "homepage": "https://github.com/alexmacarthur/slide-element",
  "scripts": {
    "start": "serve",
    "build": "microbundle -i src/index.ts -o dist/index.js --name SlideElement --no-sourcemap",
    "watch": "npm run build watch",
    "prettify": "prettier src/**/*.{ts,js} --write",
    "test": "jest src/"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "jQuery",
    "slideToggle",
    "slideUp",
    "slideDown"
  ],
  "author": "Alex MacArthur <alex@macarthur.me> (https://macarthur.me)",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/dom": "^7.30.4",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "prettier": "^2.2.1",
    "serve": "^11.3.2"
  }
}
